<!DOCTYPE html>
<html lang="en">
  <head>
    <title>Listing Page</title>

    <link rel="stylesheet" href="/vendor/normalize-4.1.1.css" type="text/css" />
    <link rel="stylesheet" href="/vendor/border-box.css" type="text/css" />
    <link rel="stylesheet" href="/styles/layout.css" type="text/css" />

    <script type="text/javascript" src="/vendor/jquery-3.0.0.js"></script>
    <script type="text/javascript" src="/scripts/app.js"></script>
  </head>

  <body>
    <h1>New Listing</h1>
    <form action="./" method="post">
      <label for="name">Name: </label><input type="text" name="name" id=""><br>
      <label for="phone">Phone: </label><input type="text" name="phone" id=""><br>
      <label for="area">Area: </label><input type="text" name="area" id=""><br>
      <label for="city">City: </label><input type="text" name="city" id=""><br>
      <label for="state">State: </label><input type="text" name="state" id=""><br>
      <label for="pincode">Pincode</label><input type="text" name="pincode" id=""><br>
      <label for="resouce">Resource: </label>
      <select name="resource" id="">
        <% for (let resource of data){ %>
          <% console.log(resource) %>
          <option value="<%= resource.name %>"><%= resource.name %></option>
        <%}%>
      </select><br>
      <button type="submit">submit</button>
    </form>
  </body>
</html>
